{
  "$schema": "http://json.schemastore.org/swcrc",
    "jsc": {
      "parser": {
        "syntax": "typescript",
        "tsx": false,
        "decorators": true,
        "dynamicImport": true
      },
      "target": "es2020",
      "paths": {
        "@routes/*": ["./src/routes/v1/*"],
       "@controllers/*": ["./src/controllers/*"],
       "@helpers/*": ["./src/helpers/*"],
       "@grpc_helpers/*": ["./src/grpc/*"]
      },
      "baseUrl": "."
    },
    
    "module": {
      "type": "commonjs"
    }
  }